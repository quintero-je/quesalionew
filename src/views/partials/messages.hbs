{{#if success_msg }}
<script> 
M.toast({html: '{{success_msg}}', classes: 'rounded toast-success',displayLength:30000})
</script>

{{/if}}

{{#if error_msg }}
<script> 
M.toast({html: '{{error_msg}}', classes: 'rounded toast-error',displayLength:30000})
</script>

{{/if}}

{{#if error }}
<script> 
M.toast({html: '{{error}}', classes: 'rounded toast-error',displayLength:30000})
</script>

{{/if}}

